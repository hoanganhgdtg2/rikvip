lốc, nếu người chơi về nhất bằng quân 2 sẽ bị tính là \"Thối 2\".\nLưu ý: Nếu chỉ còn 1 quân bài trên tay, người chơi phải \"Báo\" cho cả làng. Người chơi \ntrước của người vừa \"Báo\" sẽ phải bằng mọi giá chặn không cho người đó \"Về\" bằng \ncách phán đoán hoặc đánh ra con bài lớn nhất của mình. Nếu người đã \"Báo\" về \nnhất mà trên tay của người giữ cửa trên vẫn còn bài hoặc bộ lớn hơn nhưng không \nđánh ra thì người đó sẽ phải đền. Mức đền = Tổng số là bài mà tất cả người chơi \ncòn + 2 + Tứ quý.",20,36,false,1,-521,[239],240]],[5,898.8,765.36],[0,0.5,1]],[6,"BG",16,[[4,-522,[243],244]],[5,436,678]],[0,"title",16,[[4,-523,[245],246]],[5,85,24],[20,318,0,0,0,0,1,1,1,1]],[22,1,0,109,[247]],[112,20,25,false,false,1,1,110,[248]],[113,"Nhập nội dung ...",20,25,false,false,1,1,111,[249]],[116,100,6,30,274,275,273],[0,"ic",113,[[10,"default",1,0,1,-524,[255],256]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[0,"ic",114,[[10,"default",2,0,2,-525,[258],259]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[0,"ic",115,[[10,"default",3,0,3,-526,[261],262]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[0,"ic",116,[[10,"default",4,0,4,-527,[264],265]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[0,"ic",117,[[10,"default",5,0,5,-528,[267],268]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[0,"ic",118,[[10,"default",6,0,6,-529,[270],271]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[0,"ic",119,[[10,"default",7,0,7,-530,[273],274]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[0,"ic",120,[[10,"default",8,0,8,-531,[276],277]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[0,"ic",121,[[10,"default",9,0,9,-532,[279],280]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[0,"ic",122,[[10,"default",10,0,10,-533,[282],283]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[0,"ic",123,[[10,"default",11,0,11,-534,[285],286]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[0,"ic",124,[[10,"default",12,0,12,-535,[288],289]],[5,329,287],[0,-15,0,0,0,0,1,0.45,0.45,1]],[6,"BG",17,[[4,-536,[293],294]],[5,1280,720]],[47,"ba